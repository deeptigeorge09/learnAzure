{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "learn-adf-deepti"
		},
		"ls_learnadlsdeepti_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_learnadlsdeepti'"
		},
		"ls_learnsadeepti_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_learnsadeepti'"
		},
		"ls_ecdc_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseUrl}"
		},
		"ls_learnadlsdeepti_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://learnadlsdeepti.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "learn-adf-deepti_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_learnadlsdeepti_accountKey": {
						"value": "[parameters('ls_learnadlsdeepti_accountKey')]"
					},
					"ls_learnsadeepti_connectionString": {
						"value": "[parameters('ls_learnsadeepti_connectionString')]"
					},
					"ls_ecdc_http_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdc_http_properties_typeProperties_url')]"
					},
					"ls_learnadlsdeepti_properties_typeProperties_url": {
						"value": "[parameters('ls_learnadlsdeepti_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "learn-adf-deepti_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'learn-adf-deepti_ArmTemplate_0')]"
			]
		}
	]
}